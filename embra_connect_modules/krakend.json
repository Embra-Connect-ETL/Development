{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://www.krakend.io/schema/krakend.json",
    "title": "Schema validation for KrakenD configuration",
    "allOf": [
      {
        "$ref": "v2.8/krakend.json"
      }
    ],
    "version": 3,
    "name": "API Gateway",
    "port": 8081,
    "cache_ttl": "3600s",
    "output_encoding": "json",
    "extra_config": {
      "plugin/http-server": {
        "enabled": true,
        "name": "custom_validation_plugin"
      }
    },
    "endpoints": [
      {
        "endpoint": "/validate_token",
        "method": "POST",
        "extra_config": {
          "validator": {
            "validation_endpoint": "https://ec-auth.onrender.com/validate_token",
            "body_to_validate": true
          }
        },
        "backend": [
          {
            "url_pattern": "/validate_token",
            "host": [
              "https://ec-connect-ide-1-0.onrender.com"
            ]
          }
        ]
      },
