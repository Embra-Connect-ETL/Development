{
    "version": 3,
    "timeout": "3000ms",
    "extra_config": {
      "github_com/devopsfaith/krakend-cors": {
        "allow_origins": ["*"],
        "allow_methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
        "allow_headers": ["Authorization", "Content-Type"],
        "expose_headers": ["Content-Length"],
        "max_age": "86400",
        "allow_credentials": true
      }
    },
    "endpoints": [
      {
        "endpoint": "/register",
        "method": "POST",
        "backend": [
          {
            "url_pattern": "/register",
            "host": ["https://ec-access-control.onrender.com"]
          }
        ]
      },
      {
        "endpoint": "/login",
        "method": "POST",
        "backend": [
          {
            "url_pattern": "/login",
            "host": ["https://ec-access-control.onrender.com"]
          }
        ]
      },
      {
        "endpoint": "/update",
        "method": "PUT",
        "backend": [
          {
            "url_pattern": "/update",
            "host": ["https://ec-access-control.onrender.com"]
          }
        ]
      }
    ]
  }
  