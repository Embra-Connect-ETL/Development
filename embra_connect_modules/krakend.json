{
  "version": 3,
  "timeout": "10000ms",
  "extra_config": {
    "github_com/devopsfaith/krakend-cors": {
      "allow_origins": [
        "*"
      ],
      "allow_methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "OPTIONS"
      ],
      "allow_headers": [
        "Authorization",
        "Content-Type"
      ],
      "expose_headers": [
        "Content-Length"
      ],
      "backend/http": {
        "return_error_code": true
      },
      "router": {
        "return_error_msg": true
      },
      "qos/ratelimit/router": {
        "max_rate": 50,
        "every": "10m",
        "client_max_rate": 5
      },
      "max_age": "86400",
      "aggregate": false,
      "allow_credentials": true
    }
  },
  "endpoints": [
    {
      "endpoint": "/register",
      "method": "POST",
      "backend": [
        {
          "url_pattern": "/register",
          "host": [
            "https://ec-access-control.onrender.com"
          ]
        }
      ]
    },
    {
      "endpoint": "/login",
      "method": "POST",
      "backend": [
        {
          "url_pattern": "/login",
          "host": [
            "https://ec-access-control.onrender.com"
          ]
        }
      ]
    },
    {
      "endpoint": "/update/{userId}",
      "method": "PUT",
      "backend": [
        {
          "url_pattern": "/users/{userId}",
          "host": [
            "https://ec-access-control.onrender.com"
          ]
        }
      ]
    },
    {
      "endpoint": "/delete/{userId}",
      "method": "DELETE",
      "backend": [
        {
          "url_pattern": "/users/{userId}",
          "host": [
            "https://ec-access-control.onrender.com"
          ]
        }
      ]
    }
  ]
}